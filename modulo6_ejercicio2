class Alumno:
    def __init__(self, nombre, nota):
        self.nombre = nombre
        self.nota = nota

    def imprimir(self):
        print("Nombre: ", self.nombre)
        print("Nota: ", self.nota)

    def resultado(self):
        if self.nota >= 5:
            print("El alumno esta aprobado")
        else:
            print("El alumno ha suspendido")


alumno = Alumno("Jorge", 7)

alumno.imprimir()
alumno.resultado()
